{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "max-len": "off",
    "no-unused-vars": "off",
    "new-cap": "off",
    "require-jsdoc": "off"
  },
  "extends": ["eslint:recommended", "google"]
}